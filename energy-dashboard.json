{
  "name": "Energy Monitoring Dashboard",
  "description": "A comprehensive dashboard for monitoring energy usage and system health",
  "widgets": [
    {
      "id": "energy-consumption-chart",
      "type": "chart",
      "title": "Energy Consumption Trend",
      "x": 0,
      "y": 0,
      "w": 8,
      "h": 4,
      "minW": 4,
      "minH": 3,
      "chartType": "line",
      "timeRange": 86400000,
      "showLegend": true,
      "autoScale": true,
      "tagId": "power-meter-1"
    },
    {
      "id": "power-gauge",
      "type": "gauge",
      "title": "Current Power Usage",
      "x": 8,
      "y": 0,
      "w": 4,
      "h": 4,
      "minW": 2,
      "minH": 3,
      "gaugeType": "radial",
      "minValue": 0,
      "maxValue": 500,
      "tagId": "power-meter-1",
      "thresholds": [
        { "value": 200, "color": "#22c55e" },
        { "value": 350, "color": "#f59e0b" },
        { "value": 450, "color": "#ef4444" }
      ],
      "showValue": true,
      "showUnit": true
    },
    {
      "id": "energy-efficiency",
      "type": "numeric",
      "title": "Energy Efficiency",
      "x": 8,
      "y": 4,
      "w": 4,
      "h": 2,
      "minW": 2,
      "minH": 2,
      "tagId": "efficiency-calc-1",
      "displayFormat": "0.0 %",
      "showUnit": true,
      "trendIndicator": true
    },
    {
      "id": "system-status",
      "type": "status",
      "title": "System Status",
      "x": 8,
      "y": 6,
      "w": 4,
      "h": 2,
      "minW": 2,
      "minH": 2,
      "tagId": "machine-status-1",
      "shape": "circle",
      "statusMap": {
        "0": { "label": "Offline", "color": "#6b7280" },
        "1": { "label": "Standby", "color": "#3b82f6" },
        "2": { "label": "Running", "color": "#22c55e" },
        "3": { "label": "Warning", "color": "#f59e0b" },
        "4": { "label": "Critical", "color": "#ef4444" }
      },
      "showLabel": true
    },
    {
      "id": "power-distribution-chart",
      "type": "chart",
      "title": "Power Distribution",
      "x": 0,
      "y": 4,
      "w": 8,
      "h": 4,
      "minW": 3,
      "minH": 3,
      "chartType": "bar",
      "timeRange": 3600000,
      "showLegend": true,
      "autoScale": true
    },
    {
      "id": "energy-sources-table",
      "type": "table",
      "title": "Energy Sources",
      "x": 0,
      "y": 8,
      "w": 8,
      "h": 4,
      "minW": 4,
      "minH": 4,
      "tagIds": [],
      "columns": [
        { "key": "name", "label": "Source", "width": 150 },
        { "key": "value", "label": "Output (kW)", "width": 100 },
        { "key": "efficiency", "label": "Efficiency (%)", "width": 100 },
        { "key": "status", "label": "Status", "width": 100 }
      ],
      "pageSize": 5,
      "sortable": true,
      "defaultSortField": "value"
    },
    {
      "id": "system-alerts",
      "type": "alert",
      "title": "System Alerts",
      "x": 8,
      "y": 8,
      "w": 4,
      "h": 4,
      "minW": 3,
      "minH": 3,
      "tagId": "alarm-status-1",
      "maxAlerts": 5,
      "alertLevel": "warning",
      "showTimestamp": true,
      "showDescription": true
    }
  ]
}